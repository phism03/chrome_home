{
    "manifest_version": 3,
    "name": "Phism Home",
    "version": "1.0.0",
    "description": "最初に目にするブラウザの新規タブを、モダンで実用性のある“便利なページ”へ。",
    "action": {
        "default_popup": "index.html"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "host_permissions": [
        "https://suggestqueries.google.com/"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage",
        "history"
    ]
}